{
  "name": "term-structure-protocol",
  "version": "0.2.1",
  "description": "Term Structure Protocol",
  "license": "MIT",
  "author": "Term Structure Labs",
  "scripts": {
    "start": "hardhat node",
    "clean": "hardhat clean && rm -rf cache",
    "compile": "hardhat compile",
    "build": "hardhat compile && hardhat typechain",
    "verify": "hardhat verify --network",
    "storage:layout": "hardhat storage-layout",
    "size": "hardhat size-contracts",
    "deploy:local": "hardhat run scripts/deploy/local.ts",
    "deploy:dev_devnet": "hardhat run scripts/deploy/dev_devnet.ts",
    "deploy:staging_sepolia": "hardhat run scripts/deploy/staging_sepolia.ts",
    "deploy:testnet_sepolia": "hardhat run scripts/deploy/testnet_sepolia.ts",
    "deploy:testnet": "hardhat run scripts/deploy/testnet.ts",
    "deploy:mainnet": "hardhat run scripts/deploy/mainnet.ts",
    "diamondLoupe": "hardhat run scripts/utils/diamondLoupe.ts",
    "test": "npm run test:normal && npm run test:fork && npm run test:time-dependency",
    "test:fork": "cross-env IS_FORK_MAINNET=true hardhat test",
    "test:normal": "cross-env IS_FORK_MAINNET=false hardhat test",
    "test:time-dependency": "npm run test:rollup && npm run test:activateEvacuation && npm run test:consume && npm run test:evacuate && npm run test:restore",
    "test:deploy": "hardhat test test/zkTrueUp/deploy/deploy.test.ts",
    "test:addFacet": "hardhat test test/zkTrueUp/upgrade/addFacet.test.ts",
    "test:removeFacet": "hardhat test test/zkTrueUp/upgrade/removeFacet.test.ts",
    "test:replaceFacet": "hardhat test test/zkTrueUp/upgrade/replaceFacet.test.ts",
    "test:register": "hardhat test test/zkTrueUp/account/register.test.ts",
    "test:deposit": "hardhat test test/zkTrueUp/account/deposit.test.ts",
    "test:withdraw": "hardhat test test/zkTrueUp/account/withdraw.test.ts",
    "test:forceWithdraw": "hardhat test test/zkTrueUp/account/forceWithdraw.test.ts",
    "test:address": "hardhat test test/zkTrueUp/address/address.test.ts",
    "test:protocolParams": "hardhat test test/zkTrueUp/protocolParams/protocolParams.test.ts",
    "test:tsbToken": "hardhat test test/zkTrueUp/tsb/tsbToken.test.ts",
    "test:redeem": "hardhat test test/zkTrueUp/tsb/redeem.test.ts",
    "test:healthFactor": "hardhat test test/zkTrueUp/loan/healthFactor.test.ts",
    "test:flashLoan": "hardhat test test/zkTrueUp/flashLoan/flashLoan.test.ts",
    "test:collateral": "hardhat test test/zkTrueUp/loan/collateral.test.ts",
    "test:repay": "hardhat test test/zkTrueUp/loan/repay.test.ts",
    "test:repayAndDeposit": "hardhat test test/zkTrueUp/loan/repayAndDeposit.test.ts",
    "test:fullLiquidation": "hardhat test test/zkTrueUp/loan/fullLiquidation.test.ts",
    "test:halfLiquidation": "hardhat test test/zkTrueUp/loan/halfLiquidation.test.ts",
    "test:rollBorrow": "hardhat test test/zkTrueUp/loan/rollBorrow.test.ts",
    "test:token": "hardhat test test/zkTrueUp/token/token.test.ts",
    "test:oracleMock": "hardhat test test/zkTrueUp/testnet/oracleMock/oracleMock.test.ts",
    "test:tsFaucet": "hardhat test test/zkTrueUp/testnet/tsFaucet/tsFaucet.test.ts",
    "test:rollup": "hardhat test test/zkTrueUp-time-dependency/rollup/rollup.test.ts",
    "test:rollup-rollBorrow": "hardhat test test/zkTrueUp/rollup/rollBorrow.test.ts",
    "test:evacuate": "hardhat test test/zkTrueUp-time-dependency/rollup/evacuate.test.ts",
    "test:activateEvacuation": "hardhat test test/zkTrueUp-time-dependency/rollup/activateEvacuation.test.ts",
    "test:consume": "hardhat test test/zkTrueUp-time-dependency/rollup/consumeL1Request.test.ts",
    "test:restore": "hardhat test test/zkTrueUp-time-dependency/rollup/restore.test.ts",
    "test:roll": "cross-env IS_FORK_MAINNET=true hardhat test test/mainnetFork/roll/roll.test.ts",
    "test:wsteth": "cross-env IS_FORK_MAINNET=true hardhat test test/mainnetFork/extension/wsteth.test.ts",
    "test:sdai": "cross-env IS_FORK_MAINNET=true hardhat test test/mainnetFork/extension/sdai.test.ts",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js",
    "generate:docs": "hardhat docgen",
    "format": "prettier --write 'contracts/**/*.sol' '**/*.{js,ts}' && eslint --fix '**/*.{js,ts}'",
    "lint": "prettier --check 'contracts/**/*.sol' '**/*.ts' && eslint '**/*.{js,ts}'",
    "lint-contracts": "solhint 'contracts/**/*.sol'"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@openzeppelin/contracts": "^4.7.1",
    "@solidstate/abi": "^0.0.56",
    "@solidstate/contracts": "^0.0.56",
    "@solidstate/library": "^0.0.56",
    "@solidstate/spec": "^0.0.56",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "cross-env": "^7.0.3",
    "diamond-engraver": "^1.0.5",
    "dotenv": "^16.0.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.14.0",
    "hardhat-contract-sizer": "^2.8.0",
    "hardhat-docgen": "^1.3.0",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-spdx-license-identifier": "^2.0.3",
    "hardhat-storage-layout": "^0.1.6",
    "hardhat-tracer": "1.1.0-rc.6",
    "prettier": "^2.8.7",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.6.1",
    "@safe-global/protocol-kit": "^1.2.0",
    "@safe-global/safe-core-sdk-types": "^2.2.0",
    "circomlibjs": "^0.1.7",
    "term-structure-sdk": "^0.2.4"
  }
}
